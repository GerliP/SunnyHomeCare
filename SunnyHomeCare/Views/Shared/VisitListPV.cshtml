@model IEnumerable<SunnyHomeCare.Models.Visit>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Caretaker.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Patient.BloodType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Caretaker.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Patient.BloodType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
